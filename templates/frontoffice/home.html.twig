      <div class="col-lg-12">
        <div class="best_product_slider owl-carousel">
          <div class="single_product_item"></div>
          {% for produit in produits %}
          <div class="single_product_item m-3">
            <img
              src="{{ asset('images/' ~  produit.img) }}"
              width="300"
              height="300"
            />
            <div class="single_product_text">
              <h4>{{ produit.nom }}</h4>
              <h3>{{ produit.prix }}Dt</h3>
              <a
                href="{{ path('cart_add', { id: produit.id }) }}"
                class="add_cart"
                >+ add to cart<i class="ti-heart"></i
              ></a>
            </div>
          </div>
          {% endfor %}
        </div>
      </div>
    </div>
